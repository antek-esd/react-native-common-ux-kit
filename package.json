{
  "name": "react-native-common-ux-kit",
  "version": "0.4.0",
  "description": "dur",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "react-native-common-ux-kit.podspec",
    "!lib/typescript/example",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "release": "release-it",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn example && yarn && yarn pods",
    "storybook": "start-storybook -p 7007"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/antek-esd/react-native-common-ux-kit",
  "author": "Antek-ESD, Alexey Sergienko, Pavel Botezat <antek-esd@antek-aad.com> (https://github.com/antek-esd)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/antek-esd/react-native-common-ux-kit/issues"
  },
  "homepage": "https://github.com/antek-esd/react-native-common-ux-kit#readme",
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@commitlint/config-conventional": "^8.3.4",
    "@react-native-community/bob": "^0.15.2",
    "@react-native-community/eslint-config": "^2.0.0",
    "@release-it/conventional-changelog": "^1.1.4",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react-native": "^5.3.19",
    "@storybook/react-native-server": "^5.3.19",
    "@types/jest": "^26.0.0",
    "@types/react": "^16.9.19",
    "@types/react-native": "0.62.2",
    "@types/react-native-autocomplete-input": "^4.0.1",
    "@types/react-native-vector-icons": "^6.4.5",
    "@types/storybook__react": "^5.2.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-optional-require": "^0.3.1",
    "commitlint": "^8.3.5",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "pod-install": "^0.1.0",
    "prettier": "^2.0.5",
    "react": "16.11.0",
    "react-native": "0.62.0",
    "release-it": "^13.5.8",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint && yarn typescript"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "@react-native-community/bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "bundledDependencies": [
    "@react-native-community/netinfo",
    "@react-native-community/picker",
    "react-native-raw-bottom-sheet",
    "react-native-paper",
    "react-native-vector-icons",
    "react-native-animatable"
  ],
  "dependencies": {
    "@react-native-community/netinfo": "^5.9.3",
    "@react-native-community/picker": "^1.6.5",
    "react-native-animatable": "^1.3.3",
    "react-native-autocomplete-input": "^4.2.0",
    "react-native-paper": "^3.10.1",
    "react-native-raw-bottom-sheet": "^2.2.0",
    "react-native-vector-icons": "^6.6.0"
  }
}
